<?xml version="1.0" encoding="UTF-8" ?>
<portscope rows="1" columns="2" carrier="mcast">
    <plot gridx="0"
          gridy="0"
          hspan="1"
          vspan="1"
          title="Hand Position"
          size="201"
          minval="-20"
          maxval="40"
          bgcolor="#EEFFEE">
        <graph remote="/handController/gmmRegression:o"
               index="4"
               color="Black"
               title="Predicted Hand Position"
               type="lines"
               size="1" />
        <graph remote="/handController/gmmRegression:o"
               index="5"
               color="Red"
               title="Target Hand Position"
               type="lines"
               size="1" />
        <graph remote="/handController/gmmRegression:o"
               index="6"
               color="Blue"
               title="Actual Hand Position"
               type="lines"
               size="1" />
    </plot>
	<plot gridx="1"
          gridy="0"
          hspan="1"
          vspan="1"
          title="Grip Strength"
          size="201"
          minval="-5"
          maxval="130"
          bgcolor="#FFEEEE">
        <graph remote="/handController/gmmRegression:o"
               index="19"
               color="Red"
               title="Target Grip Strength"
               type="lines"
               size="1" />
        <graph remote="/handController/gmmRegression:o"
               index="20"
               color="Blue"
               title="Actual Grip Strength"
               type="lines"
               size="1" />
    </plot>

</portscope>
